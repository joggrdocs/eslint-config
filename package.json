{
  "name": "eslint-config-joggr",
  "version": "1.0.0",
  "description": "ESLint configuration for Joggr projects.",
  "license": "MIT",
  "main": "./dist/eslint-config-joggr.cjs",
  "module": "./dist/eslint-config-joggr.mjs",
  "types": "./dist/eslint-config-joggr.d.ts",
  "scripts": {
    "test": "vitest",
    "lint-staged": "lint-staged",
    "analyze": "yarn analyze:fmt && yarn analyze:lint && yarn analyze:types",
    "analyze:lint": "eslint --quiet src/**/*.{ts,tsx}",
    "analyze:fmt": "prettier --check .",
    "analyze:types": "tsc --noEmit",
    "fix:lint": "eslint src/**/*.{ts} --fix",
    "fix:fmt": "prettier --write .",
    "build": "vite build",
    "prepare": "husky"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-semistandard": "^17.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-plugin-filenames-simple": "^0.9.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-storybook": "^0.8.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.2",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "vite-plugin-dts": "^3.7.2",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.2"
  },
  "packageManager": "yarn@4.1.0"
}
